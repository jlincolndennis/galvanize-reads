{{#authors}}
<section class='author-panel'>
  <img class='author-headshot' src="{{url}}" alt="" />
<h3><a href="authors/{{id}}">{{first_name}} {{last_name}}</a> </h3>
<button type="button" name="button">Edit</button>
<button type="button" name="button">Delete</button>
<p>{{bio}}</p>
<div class='books'>
  <h4>Books!</h4>
  {{#titles}}
  <p>{{this}}</p>
  {{/titles}}
</section>

{{/authors}}
